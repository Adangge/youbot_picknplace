<?xml version="1.0"?>

<!--
This launch file starts a series of nodes for the Picknplace project.
MoveIt and motion planning nodes are initialized.

-->

<launch>
	<!-- move it interface -->
    <include file="$(find youbot_moveit_config)/launch/move_group.launch">
  		<arg name="publish_monitored_planning_scene" value="true" />
 	</include>

    <!-- launch motion planning and other necessary nodes -->
    <include file="$(find youbot_picknplace)/launch/motion_planning.launch" />

    <!-- launch gazebo world -->
    <!-- <include file="$(find youbot_picknplace)/launch/camera.launch" /> -->

</launch>
